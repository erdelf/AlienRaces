<Patch>
    <Operation Class="PatchOperationSequence">
        <operations>
            <li Class="PatchOperationAttributeSet">
                <xpath>Defs/ThingDef[defName="Human"]</xpath>
                <attribute>Class</attribute>
                <value>AlienRace.ThingDef_AlienRace</value>
            </li>
            <li Class="PatchOperationAttributeSet">
                <xpath>Defs/ThingDef[defName="Human"]</xpath>
                <attribute>Name</attribute>
                <value>HumanRace</value>
            </li>
            <li Class="PatchOperationAdd">
                <xpath>Defs/ThingDef[defName="Human"]</xpath>
                <value>
                    <alienRace>
                        <generalSettings>
                            <allowHumanBios>true</allowHumanBios>
                            <alienPartGenerator>
                                <aliencrowntypes>
                                    <li>Average_Normal</li>
                                    <li>Average_Wide</li>
                                    <li>Average_Pointy</li>
                                    <li>Narrow_Normal</li>
                                    <li>Narrow_Pointy</li>
                                    <li>Narrow_Wide</li>
                                </aliencrowntypes>
                                <alienskincolorgen Class="AlienRace.ColorGenerator_SkinColorMelanin">
                                    <minMelanin>0</minMelanin>
                                    <maxMelanin>1</maxMelanin>
                                </alienskincolorgen>
                                <bodyAddons></bodyAddons>
                            </alienPartGenerator>
                        </generalSettings>
                        <graphicPaths>
                            <li>
                                <skinShader>CutoutSkin</skinShader>
                            </li>
                        </graphicPaths>
                        <hairSettings></hairSettings>
                        <thoughtSettings>
                            <cannotReceiveThoughts></cannotReceiveThoughts>
                            <canStillReceiveThoughts></canStillReceiveThoughts>
                        </thoughtSettings>
                        <relationSettings>
                            <renamer></renamer>
                        </relationSettings>
                        <raceRestriction>
                            <apparelList></apparelList>
                            <whiteApparelList></whiteApparelList>
                            <weaponList></weaponList>
                            <whiteWeaponList></whiteWeaponList>
                            <buildingList></buildingList>
                            <whiteBuildingList></whiteBuildingList>
                            <recipeList></recipeList>
                            <plantList></plantList>
                            <whitePlantList></whitePlantList>
                            <traitList></traitList>
                            <whiteTraitList></whiteTraitList>
                            <foodList></foodList>
                            <petList></petList>
                            <conceptList></conceptList>
                            <workGiverList></workGiverList>
                        </raceRestriction>
                    </alienRace>
                </value>
            </li>
        </operations>
    </Operation>
</Patch>